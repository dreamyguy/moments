  "dependencies": {
    "acorn": "^6.0.0",
    "ajv": "^6.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "bootstrap-4-grid": "^2.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-css-modules": "^1.1.0",
    "jest-transform-stub": "^1.0.0",
    "moment": "^2.22.2",
