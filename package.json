  "jest": {
    "modulePaths": [
      "src",
      "node_modules"
    ],
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      ".[css,scss]": "./node_modules/jest-css-modules"
    },
    "testEnvironment": "node",
    "setupFiles": [
      "./src/test/setupTests.js"
    ]
  },
  "engines": {
    "node": ">=10.12.0"
  }
